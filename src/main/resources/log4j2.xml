<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
  <Appenders>
    <Console name="JSON_OUTPUT" target="SYSTEM_OUT">
      <JsonLayout compact="true" complete="false" eventEol="true" objectMessageAsJsonObject="true"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="JSON_OUTPUT"/>
    </Root>
  </Loggers>
</Configuration>
